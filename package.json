{
  "name": "node-red-contrib-iobroker",
  "version": "1.3.0-beta.2",
  "description": "Node-RED nodes for ioBroker integration via WebSocket. Provides input, output, get, object management, historical data access, and live log monitoring capabilities.",
  "keywords": [
    "node-red",
    "iobroker",
    "smart-home",
    "automation",
    "websocket",
    "home-automation",
    "iot",
    "smarthome"
  ],
  "author": "Marc Berg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marc-Berg/node-red-contrib-iobroker.git"
  },
  "homepage": "https://github.com/Marc-Berg/node-red-contrib-iobroker#readme",
  "bugs": {
    "url": "https://github.com/Marc-Berg/node-red-contrib-iobroker/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "dayjs": "^1.11.18",
    "express": "^4.21.2",
    "ws": "^8.18.0"
  },
  "files": [
    "nodes/*.js",
    "nodes/*.html",
    "nodes/icons/*.svg",
    "lib/**/*.js",
    "shared/*.js",
    "examples/*.json",
    "README.md",
    "README.de.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "node-red": {
    "version": ">=2.0.0",
    "nodes": {
      "iobin": "nodes/iob-in.js",
      "iobout": "nodes/iob-out.js",
      "iobinobject": "nodes/iob-inobject.js",
      "iobget": "nodes/iob-get.js",
      "iobgetobject": "nodes/iob-getobject.js",
      "iobsetobject": "nodes/iob-setobject.js",
      "iobhistory": "nodes/iob-history.js",
      "ioblog": "nodes/iob-log.js",
      "iobsendto": "nodes/iob-sendto.js",
      "iob-config": "nodes/iob-config.js"
    },
    "examples": [
      "examples/iob-get.json",
      "examples/iob-getobject.json",
      "examples/iob-setobject.json",
      "examples/iob-history.json",
