{
  "name": "node-red-contrib-iobroker",
  "version": "0.5.0",
  "description": "Node-RED nodes for ioBroker integration via WebSocket. Provides input, output, get, and object management capabilities.",
  "keywords": [
    "node-red",
    "iobroker",
    "smart-home",
    "automation",
    "websocket",
    "home-automation",
    "iot",
    "smarthome"
  ],
  "author": "Marc Berg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marc-Berg/node-red-contrib-iobroker.git"
  },
  "homepage": "https://github.com/Marc-Berg/node-red-contrib-iobroker#readme",
  "bugs": {
    "url": "https://github.com/Marc-Berg/node-red-contrib-iobroker/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "ws": "^8.18.2",
    "express": "^4.18.0"
  },
  "peerDependencies": {
    "node-red": ">=2.0.0"
  },
  "node-red": {
    "version": ">=2.0.0",
  "nodes": {
      "iobin": "iob-in.js",
      "iobout": "iob-out.js",
      "ioboutenhanced": "iob-out-enhanced.js",
      "iobget": "iob-get.js",
      "iobgetobject": "iob-getobject.js",
      "iob-config": "iob-config.js"
    },
    "examples": [
      "examples/iob-in.json",
      "examples/iob-out.json", 
      "examples/iob-get.json",
      "examples/iob-getobject.json"
    ],
    "files": [
      "*.js",
      "*.html",
      "lib/*.js",
      "shared/*.js",
      "icons/*.svg",
      "README.md",
      "LICENSE"
    ]
  }
}