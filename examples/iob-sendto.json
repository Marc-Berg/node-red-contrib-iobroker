        "x": 300,
        "y": 280,
        "wires": [["script-result"]]
    },
    {
        "id": "inject-telegram",
        "type": "inject",
        "z": "sendto-example",
        "name": "Trigger Telegram",
        "props": [
            {
                "p": "payload",
                "v": "{\"text\":\"Motion detected in living room!\",\"user\":\"admin\"}",
                "vt": "json"
            }
        ],
        "repeat": "",
        "crontab": "",
        "once": false,
        "onceDelay": 0.1,
        "topic": "",
        "x": 100,
        "y": 100,
        "wires": [["telegram-notification"]]
    },
    {
