    {
        "id": "sql-query",
        "type": "iobsendto",
        "z": "sendto-example",
        "name": "SQL Query",
        "adapter": "sql.0",
        "command": "query",
        "message": "",
        "waitForResponse": true,
        "responseTimeout": 15000,
        "server": "iobroker-config",
        "x": 300,
        "y": 220,
        "wires": [["query-result"]]
    },
    {
        "id": "javascript-execution",
        "type": "iobsendto",
